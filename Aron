   "source": [
    "import matplotlib.pyplot as plt\n",
    "df_edit = df[[\"Log GDP per capita\",\"Life Ladder\"]].dropna(how='any',axis=0)\n",
    "x = df_edit['Log GDP per capita']\n",
    "y = np.log(df_edit[\"Life Ladder\"])\n",
    "m, b = np.polyfit(x, y, 1)\n",
    "plt.plot(x, y, 'o')\n",
    "plt.plot(x, m*x + b)"
   ]
